datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id            String   @id @default(uuid())
  userId        String   // Supabase User UUID
  name          String
  type          String
  status        String   @default("stopped")
  containerId   String?
  containerName String?
  config        Json?
  createdAt     DateTime @default(now())
}
